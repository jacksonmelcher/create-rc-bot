{
  "name": "create-rc-bot",
  "version": "0.0.2-alpha.0",
  "description": "CLI tool to quickly create a RingCentral MVP bot",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "workspaces": [
    "package/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix ",
    "dev:help": "tsc && node lib/src/index.js --help",
    "dev": "tsc && node lib/src/index.js my-bot",
    "dev:no-name": "tsc && node lib/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacksonmelcher/create-rc-bot.git"
  },
  "keywords": [
    "Glip",
    "RingCentral",
    "Bot"
  ],
  "author": "Jackson Melcher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacksonmelcher/create-rc-bot/issues"
  },
  "homepage": "https://github.com/jacksonmelcher/create-rc-bot#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^4.1.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
